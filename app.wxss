/**app.wxss**/

@import 'common/helper';
@import 'common/icon';
@import 'common/layout';
@import 'common/btn';
@import 'common/gallery';
@import 'common/vant';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  font-size: 28rpx;
  background: #f9f9fa;
}

/* banner */
.banner,
.banner-item {
  width: 750rpx;
  height: 350rpx;
}

/* 搜索 */
.mod-search {
  position: relative;
  z-index: 1;
  display: flex;
  align-items: center;
}

.mod-search__field {
  display: flex;
  align-items: center;
  flex: 1;
  height: 82rpx;
  line-height: 82rpx;
  padding: 0 34rpx;
  overflow: hidden;
  border-radius: 41rpx;
  box-shadow: 0 2rpx 13rpx rgba(0, 0, 0, .13);
  background-color: #fff;
}

.mod-search__field-icon {
  margin-right: 20rpx;
}

.mod-search__field-input {
  flex: 1;
}

.mod-search__btn {
  position: relative;
  padding: 0 30rpx 0 20rpx;
  color: #7b7b7b;
}

.mod-search__btn .mod-remind-num {
  top: -20rpx;
  right: 14rpx;
}

/* 数字微标 */
.mod-remind-num {
  position: absolute;
  display: inline-block;
  min-width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  padding: 0 5rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  background-color: #c5172c;
  border-radius: 20rpx;
  box-sizing: border-box;
}

/* 小点 */
.mod-dot {
  position: absolute;
  width: 14rpx;
  height: 14rpx;
  background: #c5172c;
  border-radius: 50%;
}

/* 房产列表 */
.mod-house-list {
  padding: 15rpx 25rpx 30rpx;
}

.mod-house-item {
  background: #fff;
  border: 2rpx solid #e7e7e7;
  border-radius: 15rpx;
  overflow: hidden;
}

.mod-house-item:not(:first-child) {
  margin: 20rpx 0 0;
}

.mod-house-item__hd {
  position: relative;
  overflow: hidden;
}

.mod-house-item__hd,
.mod-house-item__hd .mhih-img {
  width: 700rpx;
  height: 350rpx;
}

.mod-house-item__hd .mhih-tle {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 66rpx;
  line-height: 66rpx;
  padding: 0 30rpx;
  font-size: 26rpx;
  color: #fff;
  background: linear-gradient(transparent, rgba(0, 0, 0, .75));
}

.mod-house-item__hd .mhih-tle .icon {
  margin: 0 10rpx 0 0;
}

.mod-house-item__bd {
  position: relative;
  padding: 35rpx 30rpx;
}

.mod-house-item__bd .mhib-status,
.mod-house-item__bd .mhib-labels .mhib-label {
  display: inline-block;
  height: 32rpx;
  line-height: 32rpx;
  padding: 0 6rpx;
  margin: 0 13rpx 0 0;
  font-size: 24rpx;
  color: #fff;
  border-radius: 6rpx;  
}

.mod-house-item__bd .mhib-status {
  background-color: #b4a078;
}

.mod-house-item__bd .mhib-labels {
  margin: 15rpx 0 0 ;
}

.mod-house-item__bd .mhib-labels .mhib-label {
  background-color: #f2cace;
  color: #c30d22;
}

.mod-house-item__bd .mhib-labels .mhib-label:nth-of-type(4n+2) {
  background-color: #f8f4e8;
  color: #b5a26f;
}

.mod-house-item__bd .mhib-labels .mhib-label:nth-of-type(4n+3) {
  background-color: #f5f5f5;
  color: #a0a0a0;
}

.mod-house-item__bd .mhib-labels .mhib-label:nth-of-type(4n+4) {
  background-color: #f9ead1;
  color: #ff7800;
}

.mod-house-item__bd .mhib-group {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 15rpx 0 0;
}

.mod-house-item__bd .mhib-types {
  margin: 0 0 -15rpx;
}

.mod-house-item__bd .mhib-types .mhib-type {
  margin: 0 15rpx 15rpx 0;
  font-size: 26rpx;
  color: #757575;
}

.mod-house-item__bd .mhib-price {
  min-width: 200rpx;
  text-align: right;
}

.mod-house-item__bd .mhib-price__num,
.mod-house-item__bd .mhib-price__unit {
  color: #c5172c;
}

.mod-house-item__bd .mhib-price__num {
  font-size: 32rpx;
  font-weight: 700;
}

.mod-house-item__bd .mhib-price__unit,
.mod-house-item__bd .mhib-price__txt {
  font-size: 24rpx;
}

/* 星星评分 */
.mod-star {
  display: inline-block;
  width: 216rpx;
  height: 36rpx;
  vertical-align: middle;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}

.mod-star--lg {
  -webkit-transform: scale(1.5);
          transform: scale(1.5);
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
}

.mod-star--sm {
  -webkit-transform: scale(.75);
          transform: scale(.75);
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
}

.mod-star--10 {
  background-position: 0 -45rpx;
}

.mod-star--20 {
  background-position: 0 -90rpx;
}

.mod-star--30 {
  background-position: 0 -135rpx;
}

.mod-star--35 {
  background-position: 0 -180rpx;
}

.mod-star--40 {
  background-position: 0 -225rpx;
}

.mod-star--45 {
  background-position: 0 -270rpx;
}

.mod-star--50 {
  background-position: 0 -315rpx;
}

/* logo */
.mod-logo {
  width: 227rpx;
  height: 82rpx;
}

/* 标签 */
.tag-list {
  margin: 0 0 -15rpx 0;
}

.tag-item {
  display: inline-block;
  width: 184rpx;
  height: 68rpx;
  line-height: 68rpx;
  margin: 0 15rpx 15rpx 0;
  font-size: 26rpx;
  text-align: center;
  background: #e5e5e5;
  border-radius: 6rpx;
  border: 2rpx solid transparent;
}

.tag-item--active {
  background: #fff;
  color: #c5172c;
  border-color: #c5172c;
}

/* 尾部联系悬浮 */
.contact-action {
  display: flex;
  align-items: center;
  padding: 10rpx 15rpx;
  background: #fff;
  border-top: 2rpx solid #d2d2d2;
}

.contact-action__item {
  flex: 1;
  padding: 0 15rpx;
  text-align: center;
}

/* tab */
.mod-tab__tle {
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}

.mod-tab__tle-item {
  position: relative;
  display: inline-block;
  padding: 0 55rpx;
  font-size: 26rpx;
  color: #9c9c9c;
}

.mod-tab__tle-item.active {
  color: #000;
}

.mod-tab__tle-item.active:after {
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 64rpx;
  height: 6rpx;
  background: #c5172c;
  border-radius: 30rpx;
  transform: translate(-50%, 0);
  content: "";
}

.mod-tab--lg .mod-tab__tle {
  height: 100rpx;
  line-height: 100rpx;
}

/* 空数据 */
.mod-empty {
  padding: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
}

/* 表单 */
.mod-form-wrap {
  padding: 0 60rpx 30rpx;
}

.mod-form-box {
  padding: 60rpx 50rpx 50rpx;
  margin: 0 auto;
  box-sizing: border-box;
  background: #fff;
  border-radius: 20rpx;
}

.mod-form-item {
  display: flex;
  align-items: center;
  padding: 25rpx 0 25rpx 10rpx;
  border-bottom: 1rpx solid #dfdfdf;
}

.mod-form-item:not(:first-of-type) {
  margin: 100rpx 0 0;
}

.mod-form-item--no-bd {
  border-bottom: 0;
}

.mod-form-item__label {
  width: 100rpx;
}

.mod-form-item__icon {
  margin: 0 30rpx 0 0;
}

.mod-form-item__field {
  flex: 1;
  overflow: hidden;
}

.mod-form--label-auto .mod-form-item__label {
  width: auto;
}

/* 媒体对象 */
.mod-media-item {
  display: flex;
  align-items: center;
  background: #fff;
}

.mod-media-item__img,
.mod-media-item__img image {
  width: 191rpx;
  height: 148rpx;
}

.mod-media-item__img {
  overflow: hidden;
  margin: 0 35rpx 0 0;
  border-radius: 10rpx;
}

.mod-media-item__bd {
  flex: 1;
  overflow: hidden;
}

.mod-media-item__bd .mmib-tle,
.mod-media-item__bd .mmib-txt,
.mod-media-item__bd .mmib-price {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.mod-media-item__bd .mmib-tle {
  font-size: 30rpx;
}

.mod-media-item__bd .mmib-txt {
  font-size: 24rpx;
  color: #5f5f5f;
}

.mod-media-item__bd .mmib-price text {
  font-size: 30rpx;
  font-weight: 700;
}

.mod-media-list--has-bd .mod-media-item {
  padding: 30rpx 0;
}

.mod-media-list--has-bd .mod-media-item:not(:first-of-type) {
  border-top: 1rpx solid #e5e5e5;
}

/* 聊天 */
.mod-chat__time {
  margin: 20rpx 0;
  text-align: center;
}

.mod-chat__time text {
  display: inline-block;
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 15rpx;
  border-radius: 20rpx;
  font-size: 20rpx;
  color: #989898;
  background: #eee;
}

.mod-chat-list {
  padding: 0 25rpx 80rpx;
}

.mod-chat-item {
  display: flex;
  align-items: flex-end;
  padding-bottom: 32rpx;
}

.mod-chat-item:not(:first-of-type) {
  margin: 40rpx 0 0;
}

.mod-chat-item__info {
  position: relative;
  margin: 0 10rpx 0 0;
}

.mod-chat-item__info .mcii-img {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.mod-chat-item__info .mcii-txt {
  position: absolute;
  bottom: -35rpx;
  left: 50%;
  width: 100rpx;
  font-size: 24rpx;
  margin: 14rpx 0 0;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  transform: translate(-50%, 0);
}

.mod-chat-item__bd {
  flex: 1;
}

.mod-chat-item__bd .mcib-txt {
  display: inline-block;
  max-width: 538rpx;
  padding: 20rpx 30rpx;
  min-height: 84rpx;
  line-height: 1.6;
  color: #4d4d4d;
  border-radius: 42rpx;
  background: #fff;
  box-shadow: 0 1rpx 13rpx rgba(210, 210, 210, .13);
  box-sizing: border-box;
}

.mod-chat-item--reverse {
  flex-direction: row-reverse;
}

.mod-chat-item--reverse .mod-chat-item__info {
  margin: 0 0 0 10rpx;
}

.mod-chat-item--reverse .mod-chat-item__bd {
  text-align: right;
}

.mod-chat-item--reverse .mod-chat-item__bd .mcib-txt {
  text-align: left;
}

.mod-chat-item--high .mod-chat-item__bd .mcib-txt {
  background: #c5172c;
  color: #fff;
  box-shadow: none;
}

.mod-chat-action {
  display: flex;
  align-items: center;
  height: 100rpx;
  border-top: 1rpx solid #e5e5e5;
  background: #fff;
  box-sizing: border-box;
}

.mod-chat-action__field {
  flex: 1;
  overflow: hidden;
}

.mod-chat-action__field .mcaf-input {
  width: 100%;
  padding: 0 25rpx 0 40rpx;
  box-sizing: border-box;
}

/* 上传图片 */
.mod-upload-field {
  position: relative;
  width: 368rpx;
  height: 368rpx;
  text-align: center;
  border: 1rpx dashed #dcdcdc;
}

.mod-upload-field__inner {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.mod-upload-field__inner .mufi-tle {
  margin: 30rpx 0 0;
  font-size: 26rpx;
  color: #787878;
}