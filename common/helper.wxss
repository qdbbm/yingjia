/** 文本截断 **/
.toe {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.toe-line-2,
.toe-line-3 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

.toe-line-2 {
  -webkit-line-clamp: 2;
}

.toe-line-3 {
  -webkit-line-clamp: 3;
}

/** 三角小图标 **/
.icon-caret {
  display: inline-block;
  width: 0;
  height: 0;
  vertical-align: middle;
}

.icon-caret--top {
  border-left: 10rpx solid transparent; 
  border-right: 10rpx solid transparent; 
  border-bottom: 10rpx solid #bfbfbf; 
}

.icon-caret--bottom {
  border-left: 10rpx solid transparent; 
  border-right: 10rpx solid transparent; 
  border-top: 10rpx solid #bfbfbf; 
}

.icon-caret--left {
  border-top: 10rpx solid transparent; 
  border-bottom: 10rpx solid transparent; 
  border-right: 10rpx solid #bfbfbf;
}

.icon-caret--right {
  border-top: 10rpx solid transparent; 
  border-bottom: 10rpx solid transparent; 
  border-left: 10rpx solid #bfbfbf;
}

.icon-caret--top-left {
  border-top: 10rpx solid #bfbfbf;
  border-right: 10rpx solid transparent;
}

.icon-caret--top-right {
  border-top: 10rpx solid #bfbfbf;
  border-left: 10rpx solid transparent;
}

.icon-caret--bottom-left {
  border-bottom: 10rpx solid #bfbfbf;
  border-right: 10rpx solid transparent;
}

.icon-caret--bottom-right {
  border-bottom: 10rpx solid #bfbfbf;
  border-left: 10rpx solid transparent;
}

