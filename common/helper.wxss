/** 固定定位 **/
.fixed-top {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}

.fixed-bottom {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}

.pos-r {
  position: relative !important;
}

/** 垂直对齐 **/
.va {
  font-size: 0;
}

.va::before {
  display: inline-block;
  height: 100%;
  vertical-align: middle;
  content: "";
}

.vam {
  vertical-align: middle;
}

.vat {
  vertical-align: top;
}

/** 遮罩层 **/
.overlay {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 4000;
  height: 100%;
  background: rgba(0,0,0,.6);
}

